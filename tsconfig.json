{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "."
  },
  "include": [
    "google-translate/pages/**/*.ts",
    "google-translate/tests/**/*.ts",
    "hotline.ua/pages/**/*.ts",
    "hotline.ua/tests/**/*.ts"
  ]
}